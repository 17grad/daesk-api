{
  "name": "17grad/daesk-php-client",
  "type": "library",
  "description": "DAESK API PHP Client",
  "keywords": ["daesk", "api"],
  "homepage": "https://dae.sk",
  "license": "MIT",
  "require": {
    "php": ">=5.6.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "authors": [
    {
        "name": "Dmitry Monin",
        "homepage": "https://dae.sk",
        "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "guzzlehttp/guzzle": "^6.5",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4" : {
      "Daesk\\Api\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": ["tests/"]
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Daesk\\Api\\Providers\\LaravelServiceProvider"
      ]
    }
  }
}
